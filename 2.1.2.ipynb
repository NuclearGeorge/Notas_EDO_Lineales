   "source": [
    "#--------------Importando paquetes------------\n",
    "import numpy as np\n",
    "from scipy.integrate import odeint\n",
    "import matplotlib.pyplot as plt\n",
    "#---------------------------------------------\n",
    "# dy/dt como función de t y y respectivamente\n",
    "def f(y,t):\n",
    "    dydt = (t - 4*t*y)/(t**2 + 1)\n",
    "    return dydt\n",
    "\n",
    "# condición inicial\n",
    "t0 = 2.0\n",
    "y0 = 1.0\n",
    "\n",
    "# tiempo\n",
    "t = np.linspace(t0,20)\n",
    "\n",
    "# Soluci de la EDO\n",
    "y = odeint(f,y0,t)\n",
    "\n",
    "# Gráfica de resultado\n",
    "plt.plot(t,y)\n",
    "plt.xlabel('t', fontsize=18)\n",
    "plt.ylabel('y(t)', fontsize=18)\n",
    "plt.show()"
   ]
  }
